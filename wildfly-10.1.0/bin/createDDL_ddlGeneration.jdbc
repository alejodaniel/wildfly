CREATE TABLE payrequest (IDPAYREQUEST INTEGER NOT NULL, AUTORIZATION varchar(30), CARDNUMBER varchar(25), CREATE_DATE DATE, CREDITCARD varchar(25), DEFEATEDDATE DATE, DISCOUNT decimal(10,2), DOCUMENT varchar(20), ESTADO varchar(20), IDFORM int(11), IDUSER int(11), ID_OWNER int(11), OBSERVATION varchar(100), PAYDATE DATE, PERSON varchar(80), SECURECODE varchar(25), SESSION varchar(15), SOLICITUD varchar(25), TOTAL decimal(10,2), TYPE varchar(20), VALUE decimal(10,2), VOUCHER_NUMBER varchar(40), USER_IDUSER INTEGER, PRIMARY KEY (IDPAYREQUEST))
CREATE INDEX IX_payrequest_FK_payrequest_USER_IDUSER ON payrequest (USER_IDUSER)
ALTER TABLE payrequest ADD CONSTRAINT FK_payrequest_USER_IDUSER FOREIGN KEY (USER_IDUSER) REFERENCES userbox (IDUSER)
